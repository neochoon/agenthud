{
  "name": "agenthud",
  "version": "0.5.0",
  "description": "CLI tool to monitor agent status in real-time. Works with Claude Code, multi-agent workflows, and any AI agent system.",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "agenthud": "dist/index.js"
  },
  "files": [
    "dist",
    "scripts"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:save": "npx tsx scripts/save-test-results.ts",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neochoon/agenthud.git"
  },
  "keywords": [
    "cli",
    "agent",
    "dashboard",
    "monitoring",
    "claude",
    "ai",
    "terminal"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neochoon/agenthud/issues"
  },
  "homepage": "https://github.com/neochoon/agenthud#readme",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "ink-testing-library": "^4.0.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "ink": "^6.6.0",
    "react": "^19.2.3",
    "yaml": "^2.8.2"
  }
}
